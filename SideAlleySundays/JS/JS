function goToSelectedVideo() {
    var searchInput = document.getElementById('tags').value;       //pull value from textbox
    var searchId = searchInput.replace(/\s/g, '').toLowerCase();      //get rid of spaces and lowercase
    var goId = document.getElementById(searchId);                     //assign 
    console.log(goId);              
    goId.scrollIntoView();                                            //scroll
 

}

